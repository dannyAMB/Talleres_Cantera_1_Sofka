


Proceso Solicitar_Datos
	
	
	Escribir "Realizar un programa el cual solicite su nombre, apellidos, edad y estaturas";
	
	
	definir nombre, apellidos  como caracter;
	definir edad como entero;
	definir estatura  Como Real;
	

	Escribir "Escribe tu nombre";
	Leer nombre;
	Escribir"Escribe tus apellidos";
	Leer apellidos;
	Escribir"Escribe tu edad";
	Leer edad;
	Escribir"Escribe tus estatura";
	Leer estatura;	

FinProceso
